{
    "name": "GoDaddy EID Plugin",
    "version": "1.0",
    "description": "Easily identify EIDs / traffic calls during a browsing session",
    "manifest_version": 2,
    "options_page": "./html/options.html",
    "devtools_page": "./html/devtools.html",
    "browser_action": {
        "default_title": "EID MUBARAK",
        "default_popup": "./html/popup.html"
    },
    "permissions": [
        "tabs",
        "storage",
        "*://*.godaddy.com/*"
    ],
    "content_scripts": [ {
      "js": [ "./src/mubarak.js" ],
      "matches": [ "*://*.godaddy.com/*" ],
      "run_at": "document_end"
    } ],
    "content_security_policy":"script-src 'self' https://account.godaddy.com; object-src 'self'"
}